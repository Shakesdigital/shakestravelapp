{
  "version": 2,
  "name": "shakestravel-app",
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "frontend/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://shakestravel-dashboard.up.railway.app",
    "NEXT_TELEMETRY_DISABLED": "1"
  }
}